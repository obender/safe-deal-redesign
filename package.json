{
  "name": "Safe-Deal",
  "version": "1.0.0",
  "description": "My majestic Nuxt.js project",
  "author": "--registry=https://registry.yarnpkg.com",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "i18n:stats": "vue-i18n-extract report -v './?(pages|layouts)/**/*.?(js|vue)' -l 'locale/texts/**/*.json'",
    "i18n:extract": "vue-i18n-extract-translations -v './?(pages|layouts)/**/*.?(js|vue)' -l './locale/texts/'  --def-locale -t en",
    "i18n:translate": "cd tools && yarn translate && cd ..",
    "build": "nuxt build",
    "start": "nuxt start",
    "dist": "nuxt generate",
    "serve": "http-server ./dist -p 3000 -o",
    "test": "yarn dist && yarn serve"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.17.3",
    "core-js": "^3.6.5",
    "nuxt": "^2.14.7",
    "nuxt-i18n": "^6.15.4",
    "popper.js": "^1.14.6",
    "vue-social-sharing": "^3.0.4",
    "vue-sweetalert2": "^4.1.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "http-server": "^0.12.3",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "sass-loader": "^10.1.0",
    "vue-i18n-extract": "^1.1.10",
    "vue-i18n-extract-translations": "^0.1.3",
    "vue-svg-loader": "^0.16.0"
  }
}
